<UserControl xmlns="https://github.com/avaloniaui"
             x:Class="H2_Gruppe_project.Views.MainMenuView"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:H2_Gruppe_project.ViewModels"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="vm:MainMenuViewModel">

	<StackPanel>
		<TextBlock Text="Welcome to the Main Menu!" />
		<Button Content="Go to Add Vehicle" Command="{Binding GoToAddVHViewCommand}" />
	</StackPanel>
</UserControl>
